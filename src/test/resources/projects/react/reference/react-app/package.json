{
  "name": "react-app",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && clientlib --verbose",
    "sync": "aemsync -d -w ../ui.apps/src/main/content",
    "test": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --coverage --no-cache --runInBand",
    "eject": "react-scripts eject",
    "deploy-local": "mvn -f ../pom.xml clean install -PautoInstallPackage"
  },
  "proxy": "http://localhost:4502",
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*",
      "!src/ImportComponents.js",
      "!src/index.js"
    ]
  },
  "dependencies": {
    "@adobe/cq-react-editable-components": "~1.1.0",
    "@adobe/cq-spa-component-mapping": "~1.0.3",
    "@adobe/cq-spa-page-model-manager": "~1.0.7",
    "aem-clientlib-generator": "^1.4.1",
    "clone": "^2.1.2",
    "dompurify": "^1.0.9",
    "history": "^4.7.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-fast-compare": "^2.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.7.0"
  },
  "devDependencies": {
    "aemsync": "^3.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "node-fetch": "^2.3.0",
    "sinon": "^7.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
